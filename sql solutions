
##Problem 2: Nth Highest Salary
CREATE FUNCTION getNthHigestSalary (@N INT)
RETURNS INT
AS
BEGIN
    DECLARE @Salary INT;
    WITH cte AS(
          SELECT *, DENSE_RANK() OVER(ORDER BY salary DESC) rnk FROM Employee
    )
    SELECT @Salary = salary FROM cte WHERE rnk = @N;
    RETURN @Salary;
END;
